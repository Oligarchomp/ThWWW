///add_end(image,line1,line2,line3,line4)
///@param image real
///@param line1 string
///@param line2 string
///@param line3 string
///@param line4 string
function add_end(im,l1,l2,l3,l4){

	ds_list_add(image_list,im);
	
	ds_list_add(line1_list,l1);
	ds_list_add(line2_list,l2);
	ds_list_add(line3_list,l3);
	ds_list_add(line4_list,l4);
}